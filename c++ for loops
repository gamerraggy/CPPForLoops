#include<iostream>
using namespace std;
int main() {


	for (int i = 0; i < 19; i++) // this will print 0-18
		cout << i << " ";

	cout << " " << endl;

	for (int k = 6; k <= 60; k += 3) // this will print out 6-60 counting by 3s
		cout << k << " ";

	cout << " " << endl;

	for (int j = 50; j >= 10; j -= 5) // prints 50-10 subtracting by 5s
		cout << j << " ";

	cout << " " << endl;

	for (int l = 2; l < 4096; l *= 2) // multiplies 2 by 2 until it reaches 2048
		cout << l << " ";

	cout << " " << endl;

	for (int h = 2048; h >= 2; h /= 2) //  divides 2048 by 2 until it reaches 2
		cout << h << " ";





}
